<% if (typeof err!= 'undefined'){%> <%err.forEach(function(err){%>
<div
  class="alert alert-warning alert-dismissible fade show text-center mb-5 rounded"
  style="height: 20%"
  role="alert"
>
  <h5><%=err.message%></h5>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<%})%> <%}%>
