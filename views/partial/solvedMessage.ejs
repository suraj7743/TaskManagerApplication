<% if (typeof solved!= 'undefined'){%> <%solved.forEach(function(solved){%>

<div class="" role="alert">
  <div class="row">
    <div class="col-3"></div>
    <div
      class="col-6 alert alert-success alert-dismissible fade show text-center mt-2 text-dark text-success"
      role=""
    >
      <i class="bi bi-check-circle-fill"></i><%=solved.message%>
      <button
        type="button"
        class="btn-close"
        data-bs-dismiss="alert"
        aria-label="Close"
      ></button>
    </div>
  </div>
</div>
<%})%> <%}%> <% solved=[]%>
